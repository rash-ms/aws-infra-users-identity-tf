{
    "policies": {
      "dev": {
        "privilege-access-policy": {
          "name": "privilege-access",
          "Version": "2012-10-17",
          "Statement": [
            {
              "Effect": "Allow",
              "Action": "*",
              "Resource": "*"
            }
          ]
        }
      },
      "prod": {
        "readonly-access-policy": {
          "name": "readonly-access",
          "Version": "2012-10-17",
          "Statement": [
            {
              "Effect": "Allow",
              "Action": [
                "ce:*",
                "cur:*",
                "budgets:ViewBudget",
                "aws-portal:ViewBilling",
                "aws-portal:ViewUsage",
                "pricing:GetProducts",
                
                "ec2:Describe*",
                "s3:Get*",
                "s3:List*",
                "s3:PutObject",
                "s3:DeleteObject",
                "dynamodb:Get*",
                "dynamodb:Query",
                "dynamodb:Scan",
                "dynamodb:List*",
                "rds:Describe*",
                "rds:StartDBInstance",
                "rds:RebootDBInstance",
                "rds:StopDBInstance",
                "lambda:InvokeFunction",
                "lambda:List*",
                "logs:Get*",
                "logs:Describe*",
                "logs:FilterLogEvents",
                "logs:CreateLogGroup",
                "logs:CreateLogStream",
                "logs:PutLogEvents",
                "logs:DescribeLogGroups",
                "logs:DeleteLogGroup",
                "cloudwatch:Get*",
                "cloudwatch:List*",
                "cloudwatch:Describe*",
                "iam:PassRole",
                "cloudformation:Describe*",
                "cloudformation:List*",
                "ecr:BatchCheckLayerAvailability",
                "ecr:GetDownloadUrlForLayer",
                "ecr:Describe*",
                "ecs:Describe*",
                "ecs:List*",
                "elasticbeanstalk:Describe*",
                "elasticbeanstalk:List*",
                "codebuild:BatchGetBuilds",
                "codebuild:List*",
                "codedeploy:BatchGet*",
                "codedeploy:Get*",
                "codedeploy:List*",
                "codepipeline:Get*",
                "codepipeline:List*",
                "ssm:Get*",
                "ssm:List*",
                "redshift:Describe*",
                "eks:Describe*",
                "eks:List*",
                "kinesis:Describe*",
                "kinesis:Get*",
                "kinesis:List*",
                "kinesis:DisableEnhancedMonitoring",
                "kinesis:DecreaseStreamRetentionPeriod",
                "kinesis:StopStreamEncryption",
                "secretsmanager:GetSecretValue",
                "secretsmanager:List*",
                "secretsmanager:Describe*",
                "events:Describe*",
                "events:List*",
                "glue:Get*",
                "glue:List*",
                "glue:StartJobRun",
                "glue:StopJobRun",
                "emr:Describe*",
                "emr:List*",
                "emr:TerminateJobFlows",
                "sns:Get*",
                "sns:List*",
                "sns:CreateTopic",
                "sns:DeleteTopic",
                "sns:CreateSubscription",
                "sns:Subscribe",
                "sns:Publish",
                "sns:Unsubscribe",
                "sns:CreateTopic",
                "sns:DeleteTopic",
                "sns:CreateSubscription",

                "iam:GetOpenIDConnectProvider",
                "iam:CreateOpenIDConnectProvider",
                "iam:UpdateOpenIDConnectProviderThumbprint",
                "iam:DeleteOpenIDConnectProvider",
                "iam:PassRole",
                "iam:AttachRolePolicy",
                "iam:CreateRole",
                "iam:DeleteRole",
                "iam:UpdateAssumeRolePolicy"
              ],
              "Resource": "*"
            }
          ]
        }
      }
    }
  }
  